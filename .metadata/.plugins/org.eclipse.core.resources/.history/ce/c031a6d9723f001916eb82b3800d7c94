package dataStructures;

public class Node<T>
{
	private T data;
	private Node<T> next;
	//TODO geowaijgoei
	
	public Node()
	{
		this(null);
	}
	
	public Node(T data)
	{
		this.data = data;
	}
	
	public T getData()
	{
		return data;
	}
	
	public Node<T> getNext()
	{
		return next;
	}
	
	public void setNext(Node<T> next)
	{
		this.next = next;
	}
}
